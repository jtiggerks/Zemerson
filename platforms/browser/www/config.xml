<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zEmerson" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>zEmerson</name>
    <description>
        Aplicativo zEmerson
    </description>
    <author email="jtiggerks@gmail.com" href="perfiljt.com.br">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="res/screen/android/screen-ldpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="res/screen/android/screen-mdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="res/screen/android/screen-xhdpi-portrait.png" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.1.1" />
    <access origin="*" />
    <access origin="*.onesignal.com" />
    <access origin="http://www.zemerson.com.br" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#1bb394" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="7000" />
    <preference name="AllowBackForwardNavigationGestures" value="true" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="splash-screen-duration" value="3000" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="android-build-tool" value="gradle" />
</widget>
